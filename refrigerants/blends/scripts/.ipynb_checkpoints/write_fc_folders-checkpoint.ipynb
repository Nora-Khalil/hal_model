   "source": [
    "import os\n",
    "import re\n",
    "\n",
    "directory = '../'\n",
    "\n",
    "list_of_blends = [file for file in os.listdir(directory) if re.match('[A-Z0-9]+_[A-Z0-9]+', file)]\n",
    "\n",
    "for blend in list_of_blends: \n",
    "    os.makedirs(f'../{blend}/fc/error', exist_ok=True)\n",
    "    os.makedirs(f'../{blend}/fc/output', exist_ok=True)\n"
   ]
